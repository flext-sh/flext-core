from collections.abc import Callable as Callable
from typing import TypeVar

T = TypeVar("T")
U = TypeVar("U")

class FlextResult[T]:
    @property
    def is_success(self) -> bool: ...
    @property
    def success(self) -> bool: ...
    @property
    def is_failure(self) -> bool: ...
    @property
    def data(self) -> T: ...
    @property
    def error(self) -> str | None: ...
    @classmethod
    def ok(cls, data: T) -> FlextResult[T]: ...
    @classmethod
    def fail(
        cls,
        error: str,
        error_code: str | None = ...,
        error_data: dict[str, object] | None = ...,
    ) -> FlextResult[T]: ...
    def map[U](self, func: Callable[[T], U]) -> FlextResult[U]: ...
    def flat_map[U](self, func: Callable[[T], FlextResult[U]]) -> FlextResult[U]: ...
    def unwrap(self) -> T: ...
    def __bool__(self) -> bool: ...

def safe_call[T](func: Callable[[], T]) -> FlextResult[T]: ...
